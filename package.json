{
  "name": "mern-ai-room-design-generator",
  "version": "1.0.0",
  "description": "A full-stack MERN application for AI-powered room design generation",
  "main": "backend/server.js",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"cd backend && npm start\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm run dev"
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "express",
    "mongodb",
    "ai",
    "room-design",
    "interior-design",
    "gemini-ai"
  ],
  "author": "MERN Stack Developer",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/mern-ai-room-design-generator.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mern-ai-room-design-generator/issues"
  },
  "homepage": "https://github.com/your-username/mern-ai-room-design-generator#readme"
}

